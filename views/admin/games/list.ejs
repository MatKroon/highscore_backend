

  <!DOCTYPE html>
  <html>
    <%- include('../includes/header.ejs'); %>
   <body>
       <div class="wrapper">
    <%- include('../includes/sidebar.ejs'); %>
    <div id="content">
<table class="table table-bordered">
    <thead>
      <tr>
        <th scope="col">Id</th>
        <th scope="col">Spelnamn</th>
        <th scope="col">Genre</th>

      </tr>
    </thead>
    <tbody>
        
        <% games.forEach(game => { %>
            <tr>
                <th scope="row"><%= game.id %></th>
                <td><%= game.name %></td>
                <td><%= game.genre %></td>
               
              </tr>
            
       <% }); %> 
        
    </tbody>
  </table>
</div>
</div>
  </body>
  </html>